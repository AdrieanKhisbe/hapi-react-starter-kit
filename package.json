{
  "name": "hapi-react-starter-kit",
  "version": "0.0.1",
  "description": "A hapi React Starter kit with react-router, redux, react-transform",
  "main": "index.js",
  "scripts": {
    "test": "set NODE_ENV=test&& mocha --require ./test/test-init --compilers js:babel/register --recursive",
    "cover": "set NODE_ENV=test&& babel-node ./node_modules/isparta/bin/isparta cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec --require ./test/test-init --recursive&& copy ./coverage/lcov.info | cscript ./node_modules/codacy-coverage/bin/codacy-coverage.js -t 8ecb926cf1c54fb78ee16fd09b7bba5e",
    "clean": "rimraf ./static/assets",
    "start": "node index.js",
    "dev": "SET NODE_ENV=development&& npm start",
    "prod": "SET NODE_ENV=production&& SET PORT=8080&& npm start",
    "build": "rimraf ./static/assets && webpack -p --progress --stats --config ./webpack/prod.config.js",
    "postinstall": "npm run build",
    "eslint": "eslint .",
    "jscs": "jscs ."
  },
  "author": "Dindaleon",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dindaleon/hapi-react-starter-kit.git"
  },
  "homepage": "https://github.com/Dindaleon/hapi-react-starter-kit",
  "keywords": [
    "hapi",
    "react",
    "reactjs",
    "react-router",
    "react-transform",
    "es6",
    "boilerplate",
    "starterkit",
    "isomorphic",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "openshift",
    "babel"
  ],
  "license": "MIT",
  "dependencies": {
    "h2o2": "^4.0.1",
    "hapi": "^11.0.0",
    "history": "^1.12.5",
    "inert": "^3.1.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^1.0.0-rc3",
    "react-transmit": "^3.0.7"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-jscs": "^2.0.4",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "chai": "^3.3.0",
    "chalk": "^1.1.1",
    "codacy-coverage": "^1.1.3",
    "eslint": "^1.7.0",
    "eslint-plugin-react": "^3.5.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "hapi-webpack-plugin": "^1.2.1",
    "isparta": "^3.1.0",
    "jscs": "^2.3.2",
    "json-loader": "^0.5.3",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "^1.0.0",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0",
    "stats-webpack-plugin": "^0.2.2",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "engines": {
    "node": ">= 4.1.1",
    "npm": ">= 1.0.0"
  },
  "private": true
}
